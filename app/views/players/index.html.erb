<%= render partial: "nav" %>

<h2>Players</h2>

<table>
  <tr>
    <th>Name</th>
    <th>Age</th>
    <th>Team</th>
  </tr>
  <% @players.each do |player| %>
  <tr>
    <td><%= player.name %></td>
    <td><%= player.age %></td>
    <td><%=player.team.name if player.team %></td>
  </tr>
  <%end %>
</table>